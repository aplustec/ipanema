@charset "utf-8";

html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,font,img,ins,kbd,q,s,samp,small,strike,/*strong,*/sub,sup,tt,var,/*b,u,i,*/center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td{margin:0;padding:0;border:0;outline:0;font-size:100%;vertical-align:baseline;background:transparent}body{line-height:1}ol,ul{list-style:none}blockquote,q{quotes:none}:focus{outline:0}ins{text-decoration:none}del{text-decoration:line-through}table{border-collapse:collapse;border-spacing:0}

@font-face {
font-family: 'MelbourneRegular';
	src: url('melbourne-regular-webfont.eot');
	src: local('â˜º'),
	url('melbourne-regular-webfont.woff') format('woff'),
	url('melbourne-regular-webfont.ttf') format('truetype'),
	url('melbourne-regular-webfont.svgz#MelbourneRegular') format('svg'),
	url('melbourne-regular-webfont.svg#MelbourneRegular') format('svg');
font-weight: normal;
font-style: normal;
}

/* fonts */

body {
font-family: 'MelbourneRegular';
font-weight: normal;
font-style: normal;
}

html, body {width: 100%; height: 100%;}
body {cursor: default; background: #ffffff; overflow-x: hidden;}
body img {
-webkit-user-select: none;
-khtml-user-select: none;
-moz-user-select: none;
-o-user-select: none;
user-select: none;
}
::selection {background: #30aacb; color: #ffffff;}
::-moz-selection {background: #30aacb; color: #ffffff;}
img::selection {background: transparent;}
img::-moz-selection {background: transparent;}

#wr {float: left; display: inline; width: 100%; text-align: center; background: url(../img/random/rotator.php) top center no-repeat;}
.center {margin: 0 auto; width: 940px; text-align: left;}
.hd {display: none;}

#ipanema, #cursos, #inscreva-se, #duvidas, #footer {float: left; display: inline; width: 100%; text-align: center;}

#ipanema {height: 700px; background: url(../img/bg-ipanema.jpg) center bottom no-repeat; padding: 0 0 100px 0;}
#ipanema .quote {float: left; display: inline; color: #0eb693; text-transform: uppercase; width: 380px !important; padding: 20px 0 0 20px; margin: 20px 0 0 0; font-family: Arial, Helvetica, sans-serif;}
#ipanema .quote span {float: left; display: inline; width: 270px; margin: 0 0 0 14px; letter-spacing: -1px; font-weight: normal; font-size: 22px; line-height: 35px;}
#ipanema .quote span b {font-size: 26px; line-height: 35px;}
#ipanema .quote .quote1 {float: left; display: inline;}
#ipanema .quote .quote2 {float: right; display: inline; margin: 350px 0 0 0;}
#ipanema .txt {float: left; display: inline; width: 540px; font-family: Arial, Helvetica, sans-serif;}
#ipanema p {float: left; display: inline; font-size: 16px; line-height: 22px; color: #636363; font-weight: normal; padding: 20px 0 0 0; width: 540px; font-family: Arial, Helvetica, sans-serif;}

#cursos p {float: left; display: inline; font-size: 16px; line-height: 22px; color: #636363; font-weight: normal; padding: 20px 0 0 0; width: 620px; font-family: Arial, Helvetica, sans-serif;}

#inscreva-se {min-height: 1400px; background: url(../img/bg2.jpg) top center no-repeat;}
#inscreva-se p {float: left; display: inline; font-size: 16px; line-height: 22px; color: #636363; font-weight: normal; padding: 20px 0 0 0; width: 480px; font-family: Arial, Helvetica, sans-serif !important;}

#duvidas p {float: left; display: inline; font-size: 16px; line-height: 22px; color: #636363; font-weight: normal; padding: 20px 0 0 0; width: 680px; clear: both; font-family: Arial, Helvetica, sans-serif !important;}

#footer {margin: 50px 0 50px 0;}
#footer .center {border-top: 1px solid #ebebeb; padding: 10px 0 10px 0;}
#footer .center h6 {color: #c7c7c7; float: right; display: inline; text-align: right;}

/* bar */

#bar {
float: left;
display: none;
width: 100%;
position: fixed;
top: 0;
left: 0;
z-index: 999;
}
#bar .center div {float: left; display: inline; background: #ffffff; border-bottom: 1px solid #7cd3ea; width: 940px;}

#bar h1.logo a {
float: left;
display: inline;
background: url(../img/logo-bar.png) 0 0 no-repeat;
width: 130px;
height: 45px;
text-indent: -999999px;
margin: 10px;
}

#bar .mn {float: right; display: inline; margin: 18px 10px 0 0; width: auto;}
#bar .mn li {
float: left;
display: inline;
font-size: 20px;
line-height: 20px;
color: #b3b3b3;
text-decoration: none;
margin: 0 0 0 40px;
}

#bar .mn a {text-decoration: none; color: #b3b3b3; font-weight: bold;}
#bar .mn a:hover, #bar .mn a.active {color: #0eb693;}

/* header */

#header {
float: left;
display: inline;
width: 100%;
height: 1050px;
}

#header h1.logo a {
float: left;
display: inline;
background: url(../img/logo.png) 0 0 no-repeat;
width: 225px;
height: 90px;
text-indent: -999999px;
margin: 30px 0 0 0;
}

#header h2.join {float: right; display: inline; clear: both; width: 380px; margin: 470px 0 0 0;}
#header h2.join a {
float: left;
display: inline;
background: #1ed6c8;
color: #ffffff;
font-size: 65px;
line-height: 65px;
font-weight: bold;
width: 380px;
padding: 5px 0 2px 0;
text-align: center;
text-transform: uppercase;
text-decoration: none;
-moz-border-radius-topleft: 10px;
-moz-border-radius-topright: 10px;
-moz-border-radius-bottomright: 10px;
-moz-border-radius-bottomleft: 0px;
-webkit-border-radius: 10px 10px 10px 0px;
border-radius: 10px 10px 10px 0px;
}
#header h2.join a:hover {background: #00b2d9;}
#header h2.join span {float: left; display: inline; font-size: 20px; line-height: 20px; color: #1ed6c8; text-transform: uppercase; margin: 10px 0 0 10px;}

/* menu */

.mn {float: right; display: inline; width: 330px; margin: 10px 0 0 0;}
.mn li {
float: left;
display: inline;
font-size: 18px;
line-height: 25px;
color: #737373;
text-decoration: none;
margin: 0;
}
.mn li.um {width: 325px; padding: 0 0 0 5px;}
.mn li.dois {width: 270px; padding: 0 0 0 30px;}
.mn li.tres {width: 245px; padding: 0 0 0 55px;}
.mn li.quatro {width: 220px; padding: 0 0 0 80px;}
.mn li.cinco {width: 195px; padding: 0 0 0 105px;}

.mn a {text-decoration: none; color: #737373; font-weight: bold;}
.mn a:hover, .mn a.active {color: #0eb693; font-weight: bold;}

/* share */

.share {float: right; display: inline; margin: 0 10px 0 10px;}
.share li {
float: left;
display: inline;
}
.share li a {float: left; display: inline; width: 26px; height: 26px; text-indent: -999999px; margin: 16px 0 0 10px;}
.share li a.ico-facebook {background: url(../img/ico-facebook.png) top left no-repeat;}
.share li a.ico-twitter {background: url(../img/ico-twitter.png) top left no-repeat;}

/* share */

.mn-share {float: right; display: inline; clear: both; width: 390px; margin: 42px 0 0 0;}
.mn-share li {
float: left;
display: inline;
font-size: 20px;
line-height: 20px;
color: #737373;
text-decoration: none;
text-transform: uppercase;
margin: 0;
}
.mn-share li.full {width: 390px; text-indent: 20px; margin: 0 0 10px 0;}

.mn-share li a {
float: left;
display: inline;
color: #ffffff;
text-decoration: none;
width: 175px;
margin: 0 0 0 10px;
padding: 5px 10px 5px 0;
-moz-border-radius-topleft: 10px;
-moz-border-radius-topright: 10px;
-moz-border-radius-bottomright: 10px;
-moz-border-radius-bottomleft: 0px;
-webkit-border-radius: 10px 10px 10px 0px;
border-radius: 10px 10px 10px 0px;
text-align: right;
}
.mn-share a.bt-twitter {background: #35b4d5;}
	.mn-share a.bt-twitter:hover {background: #61c6e0;}
.mn-share a.bt-facebook {background: #405899;}
	.mn-share a.bt-facebook:hover {background: #5670b6;}
	
/* titulo blocos */

#ipanema h1, #cursos h1, #inscreva-se h1, #duvidas h1 {
float: left;
display: inline;
clear: both;
width: 100%;
font-size: 65px;
line-height: 65px;
font-weight: bold;
color: #00b2d9;
text-transform: uppercase;
text-indent: -3px;
margin: 100px 0 0 0;
}
#inscreva-se h1 {margin: 140px 0 0 0;}

/* faq */

#accordion {margin: 20px 0 20px 0; float: left; display: inline; font-family: Arial, Helvetica, sans-serif !important;}
#accordion h3 {font-weight: bold; font-size: 18px; line-height: 18px;}
#accordion .ui-accordion-content-active a:hover {text-decoration: none;}

/* table */

#div-01, #div-02, #div-03, #div-04, #div-05, #div-06, #div-07, #div-08, #div-09, #div-10 {float: left; display: none; width: 100%;}
#close-01, #close-02, #close-03, #close-04, #close-05, #close-06, #close-07, #close-08, #close-09, #close-10 {display: none;}

.hold-table {float: left; display: inline; width: 100%; padding: 20px 0 20px 20px; border-top: 1px dotted #ffffff; line-height: 21px; margin: 0;}
.hold-table table {margin: 0;}
.hold-table .fl {float: left; display: inline; width: 435px; margin: 0;}
.hold-table .fl.full {float: left; display: inline; width: 880px; margin: 0;}
.hold-table .fl.margin {margin: 0 10px 0 0;}

table {width: 100%; margin: 0 0 20px 0; color: #ffffff; font-family: Arial, Helvetica, sans-serif !important;}
table td {border-bottom: 1px dotted #1d6455; padding: 5px;}
table td.bg {background: #2aa188; color: #1d6455;}

/* cursos */

ul.tabs {float: left; display: inline; width: 100%; margin: 20px 0 40px 0;}
ul.tabs li {float: left; display: inline;}
ul.tabs li.full {float: left; display: inline; width: 100%; color: #0eb693; font-size: 26px; line-height: 28px; font-family: Arial, Helvetica, sans-serif; margin: 30px 0 10px 0; text-transform: uppercase; font-weight: bold;}
ul.tabs li a {
float: left;
display: inline;
overflow: hidden;
color: #00b2d9;
text-decoration: none;
width: 116px;
padding: 15px 20px 10px 20px;
font-size: 18px;
line-height: 24px;
font-weight: normal;
height: 119px;
margin: 0 1px 0 0;
background: url(../img/bg-off.gif) bottom right no-repeat;
font-family: Arial, Helvetica, sans-serif;
}
ul.tabs li a.tab10 {width: 115px; margin: 0 !important;}
ul.tabs li.active a, ul.tabs li a:hover {background: #0eb693 url(../img/bg-cursos.jpg) top left repeat-x; color: #ffffff !important;}

.tab_container {float: left; display: inline; clear: both; width: 100%; margin: 0; background: #0eb693;}
.tab_content {float: left; display: none; width: 100%;}
.tab_content p {padding: 0 0 20px 20px !important; color: #ffffff !important; width: 900px; font-family: Arial, Helvetica, sans-serif !important; line-height: 21px; clear: both;}

.tab_content h2 {color: #ffffff !important; padding: 20px 0 0 20px !important; clear: both; font-size: 40px; line-height: 40px; font-family: Arial, Helvetica, sans-serif !important; font-weight: normal;}

/* form */

.form {
float: left;
display: inline;
width: 940px;
margin: 0;
padding: 40px 0 0 0;
background: url(../img/bg-duvidas.jpg) 0 0 no-repeat;
}
#inscreva-se .form {background: none;}

.form {height: 320px;}
.form fieldset {float: left; display: inline; margin: 0 0 10px 0; width: 460px; clear: both;}
.form fieldset.half {width: 225px !important; clear: none; margin: 0 10px 0 0;}
.form fieldset.half.no-margin {width: 225px; clear: none; margin: 0;}

.form fieldset.half input {width: 203px !important;}
.form fieldset.half input.button {width: 225px !important; margin: 0;}

.form fieldset input, .form fieldset textarea, .form fieldset select {
float: left;
display: inline;
width: 438px;
font-size: 14px;
line-height: 21px;
padding: 10px;
color: #636363;
text-transform: uppercase;
font-family: Arial, Helvetica, sans-serif !important;
margin: 0;
border: 1px solid #7dd1e2;
}
.form fieldset select {text-align: left !important; height: 42px; text-transform: uppercase !important; line-height: 34px !important; width: 460px;}
.form fieldset label {font-family: Arial, Helvetica, sans-serif !important; font-size: 13px; color: #00b2d9;}
.form fieldset label i {color: #636363;}
.form fieldset.half select {width: 225px;}

.form fieldset textarea {min-width: 418px; max-width: 438px;}

.form fieldset#field-pos{ display: none;}

.form fieldset .button {
float: right;
display: inline;
color: #ffffff;
text-decoration: none;
width: 225px;
line-height: 22px;
margin: 0 0 0 10px;
padding: 9px 10px 9px 10px;
font-family: Arial, Helvetica, sans-serif !important;
background: #00b2d9;
cursor: pointer;
text-align: right;
}
.form fieldset .button:hover {background: #7dd1e2;}

.regulamento-txt {display: none; margin: 55px 0 0 0; float: left; border-left: 1px solid #00b2d9;}
.regulamento-txt p {font-size: 13px !important; line-height: 18px !important; padding: 0 40px 10px 20px !important; width: 460px !important;}
.regulamento, .regulamento-close {font-family: Arial, Helvetica, sans-serif !important; color: #00b2d9; font-size: 13px; line-height: 13px; text-transform: uppercase; text-decoration: none; margin: 14px 0 0 0; float: left;}
.regulamento:hover {color: #444444;}

.regulamento-close {
float: left;
display: inline;
color: #ffffff;
text-decoration: none;
width: 225px;
line-height: 22px;
margin: -1px 0 -10px -20px;
padding: 8px 10px 8px 10px;
font-family: Arial, Helvetica, sans-serif !important;
background: #00b2d9;
cursor: pointer;
text-align: right;
}
.regulamento-close:hover {color: #ffffff; background: #7dd1e2;}

.tab_content .bt-plus {
margin: 10px 0 20px 0;
background: #ffffff url(../img/ico-plus.gif) 8px 9px no-repeat;
padding: 10px 10px 10px 35px;
color: #0eb693;
text-decoration: none;
float: left;
display: inline;
font-family: 'MelbourneRegular';
font-weight: bold;
font-style: normal;
text-transform: uppercase;
font-size: 20px;
line-height: 20px;
}
.tab_content .bt-plus:hover {color: #1d6455;}

.bt-close, .bt-close2 {
margin: 10px 0 20px 0;
background: #ffffff url(../img/ico-close.gif) 8px 9px no-repeat;
padding: 10px 10px 10px 35px;
color: #0eb693;
text-decoration: none;
float: left;
display: inline;
font-family: 'MelbourneRegular';
font-weight: bold;
font-style: normal;
text-transform: uppercase;
font-size: 20px;
line-height: 20px;
}
.bt-close2 {float: right; margin: 5px 0 0 0; text-indent: -999999px; position: relative; z-index: 1002;}
.bt-close:hover, .bt-close2:hover {color: #1d6455;}

